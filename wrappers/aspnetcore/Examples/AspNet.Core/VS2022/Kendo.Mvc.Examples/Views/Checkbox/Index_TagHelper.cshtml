        </li>
        <li>
            <kendo-checkbox name="eq2" checked="true" enabled="false" label="Leather trim"> </kendo-checkbox>
        </li>
        <li>
            <kendo-checkbox name="eq3" label="Luggage compartment cover"> </kendo-checkbox>
